{
  "extends": [
    "../../../tsconfig.base.json"
  ],
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist/",
    "lib": [
      "es2021",
      "DOM"
    ],
    "plugins": [
      {
        "transform": "typia/lib/transform"
      }
    ],
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./tests/**/*",
    "./dist/**/*"
  ],
  "baseUrl": "./src"
}
