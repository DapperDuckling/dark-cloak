{
  "name": "keycloak-connector-server",
  "version": "0.0.27",
  "description": "Middleware connecting Node.js express or fastify servers to Keycloak",
  "keywords": [
    "express keycloak",
    "fastify keycloak",
    "node server keycloak"
  ],
  "author": "DapperDuckling Dev LLC",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/DapperDuckling/keycloak-connector/issues"
  },
  "homepage": "https://github.com/DapperDuckling/keycloak-connector#readme",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test:start:fastify": "npm run build && cd ./tests/integration/fastify/ && nodemon ./fastify-server.ts",
    "test:start:redis-cluster": "npm run build && cd ./tests/integration/redis-cluster/ && nodemon ./main.test.ts",
    "postinstall": "ts-patch install"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express-serve-static-core": "^4.17.34",
    "@types/fastify-static": "^2.2.1",
    "@types/jest": "^29.5.1",
    "keycloak-connector-server-cluster-redis": "file:../cluster-redis",
    "nodemon": "^3.0.1",
    "pino-http": "^8.4.0",
    "readline-sync": "^1.4.10",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "jose": "^4.14.4",
    "lru-cache": "^10.0.1",
    "openid-client": "^5.4.0",
    "typia": "^4.2.3"
  }
}
