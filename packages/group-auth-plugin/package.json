{
  "name": "@dapperduckling/keycloak-connector-group-auth-plugin",
  "version": "0.0.33",
  "description": "A plugin enabling Keycloak group permission management via Keycloak groups",
  "keywords": [
    "keycloak group permissions",
    "node keycloak group authorization"
  ],
  "author": "DapperDuckling Dev LLC",
  "license": "AGPL-3.0-only",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/DapperDuckling/keycloak-connector/issues"
  },
  "homepage": "https://github.com/DapperDuckling/keycloak-connector#readme",
  "scripts": {
    "build": "tsc && cpy ./src/**/*.d.ts ./dist",
    "prepublishOnly": "npm run build",
    "test:start:fastify": "npm run build && cd ./tests/integration/fastify/ && nodemon ./fastify-server.ts",
    "test:start:express": "npm run build && cd ./tests/integration/express/ && nodemon ./express-server.ts",
    "test": "node --experimental-vm-modules --require ../../suppress-experimental-vm-warnings.cjs ../../node_modules/jest/bin/jest.js --verbose --selectProjects keycloak-connector-group-auth-plugin",
    "test:vitest": "vitest",
    "test:watch": "node --experimental-vm-modules --require ../../suppress-experimental-vm-warnings.cjs ../../node_modules/jest/bin/jest.js --verbose --runInBand --watch --coverage=false --no-cache --selectProjects keycloak-connector-group-auth-plugin"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./express": "./dist/express/index.js",
    "./fastify": "./dist/fastify/index.js"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.1",
    "cpy": "^10.1.0"
  },
  "dependencies": {
    "@dapperduckling/keycloak-connector-server": "file:../server",
    "fastify-plugin": "^4.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a0bb8267752d5cdb1f239e55c310b12f6ed9f062"
}
