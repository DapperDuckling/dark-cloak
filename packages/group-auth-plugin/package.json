{
  "name": "@dapperduckling/keycloak-connector-group-auth-plugin",
  "version": "0.0.11-alpha.0",
  "description": "A plugin enabling Keycloak group permission management via Keycloak groups",
  "keywords": [
    "keycloak group permissions",
    "node keycloak group authorization"
  ],
  "author": "DapperDuckling Dev LLC",
  "license": "AGPL-3.0-only",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/DapperDuckling/keycloak-connector/issues"
  },
  "homepage": "https://github.com/DapperDuckling/keycloak-connector#readme",
  "scripts": {
    "build": "tsc && cpy ./src/**/*.d.ts ./dist",
    "prepublishOnly": "npm run build",
    "test:start:fastify": "npm run build && cd ./tests/integration/fastify/ && nodemon ./fastify-server.ts",
    "test:start:express": "npm run build && cd ./tests/integration/express/ && nodemon ./express-server.ts"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": "./dist/*",
    "./express": "./dist/express/index.js",
    "./fastify": "./dist/fastify/index.js"
  },
  "devDependencies": {
    "cpy": "^10.1.0"
  },
  "dependencies": {
    "@dapperduckling/keycloak-connector-server": "^0.0.6-alpha.0",
    "fastify-plugin": "^4.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "e68cb11d18dc1cd1b48bc0e3be25f674f9295cb3"
}
